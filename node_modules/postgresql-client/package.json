{
  "name": "postgresql-client",
  "description": "Professional PostgreSQL client for JavaScript and TypeScript",
  "version": "1.21.2",
  "author": "Panates",
  "contributors": [
    "Eray Hanoglu <e.hanoglu@panates.com>"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/panates/postgresql-client.git"
  },
  "keywords": [
    "javascript",
    "typescript",
    "ts",
    "pg",
    "postgre",
    "postgres",
    "postgresql",
    "client",
    "database",
    "rdbms"
  ],
  "dependencies": {
    "debug": "^4.3.1",
    "doublylinked": "^2.4.3",
    "lightning-pool": "^3.1.0",
    "postgres-bytea": "^3.0.0",
    "putil-merge": "^3.6.4",
    "putil-promisify": "^1.8.2",
    "putil-taskqueue": "^2.5.1",
    "putil-varhelpers": "^1.6.0"
  },
  "devDependencies": {
    "@types/debug": "^4.1.5",
    "@types/mocha": "^9.0.0",
    "@types/node": "^16.4.8",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "babel-eslint": "^10.1.0",
    "dotenv": "^10.0.0",
    "eslint": "^8.0.0",
    "eslint-config-google": "^0.14.0",
    "mocha": "^9.0.1",
    "npm-run-path": "^5.0.1",
    "nyc": "^15.1.0",
    "ts-cleanup": "^0.2.2",
    "ts-loader": "^9.0.0",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.1.5"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "engines": {
    "node": ">= 10.0"
  },
  "directories": {
    "lib": "dist",
    "test": "test"
  },
  "files": [
    "LICENSE",
    "README",
    "dist/"
  ],
  "nyc": {
    "temp-dir": "./coverage/.nyc_output"
  },
  "scripts": {
    "test": "TS_NODE_PROJECT='./test/tsconfig.json' mocha -r ts-node/register --reporter spec test/**/*.spec.ts",
    "cover": "nyc --reporter=cobertura --reporter html --reporter text npm run test",
    "clean": "npm run clean:src && npm run clean:dist",
    "clean:dist": "ts-cleanup -d dist --remove-dirs --all",
    "clean:src": "ts-cleanup -s src --all | ts-cleanup -s test",
    "prebuild": "npm run clean && npm run lint",
    "build": "tsc -b tsconfig-build.json",
    "compile": "tsc -b tsconfig.json",
    "lint": "eslint src/** --no-error-on-unmatched-pattern",
    "travis-cover": "nyc --reporter lcovonly npm run test"
  }
}
